{
  "name": "@dparish/airbrake-js-lite",
  "version": "0.0.5",
  "description": "Various utilities for externally-facing apps",
  "private": false,
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/dparish/airbrake-js-lite.git",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "lint-ci": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0 --no-fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write ./"
  },
  "devDependencies": {
    "@types/node": "^18.7.9",
    "@types/request": "2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^22.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.0",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "error-stack-parser": "^2.1.4"
  }
}
